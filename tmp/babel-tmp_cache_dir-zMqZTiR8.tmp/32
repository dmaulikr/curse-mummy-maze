"use strict";

var _classCallCheck = function (instance, Constructor) { if (!(instance instanceof Constructor)) { throw new TypeError("Cannot call a class as a function"); } };

define("states/coin", ["exports"], function (exports) {

  "use strict";

  var TILE_WIDTH = 40;
  var TILE_HEIGHT = 40;

  var Coin = function Coin(game, x, y) {
    _classCallCheck(this, Coin);

    this.sprite = game.add.sprite(x * TILE_WIDTH, y * TILE_HEIGHT, "coin", "coin.png");
  };

  exports["default"] = Coin;
});